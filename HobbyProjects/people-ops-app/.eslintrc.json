{
    "extends": ["react-app", "plugin:prettier/recommended"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "singleQuote": true,
                "trailingComma": "es5",
                "tabWidth": 4,
                "printWidth": 80
            }
        ]
    }
}
